<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>

<BorderPane xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <center>
        <AnchorPane>
            <TableView fx:id="Bookings" minWidth="-Infinity" prefHeight="280.0" prefWidth="468.0">
                <columns>
                    <TableColumn fx:id="bookingID" prefWidth="89.0" text="BookingID" />
                    <TableColumn fx:id="concertID" prefWidth="105.0" text="eventID" />
                    <TableColumn fx:id="bookingPrice" prefWidth="125.0" text="ticketType" />
                    <TableColumn fx:id="bookingDate" prefWidth="146.0" text="Quantity" />
                </columns>
            </TableView>
        </AnchorPane>
    </center>
    <left>
        <VBox alignment="TOP_CENTER" prefHeight="280.0" prefWidth="168.0" BorderPane.alignment="CENTER">
            <JFXButton fx:id="handle1" text="Load" />
            <JFXButton fx:id="handle2" prefHeight="28.0" prefWidth="150.0" text="Is it really my bookings ?" />
        </VBox>
    </left>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </padding>
</BorderPane>
