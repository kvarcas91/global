<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<BorderPane xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <center>
        <ScrollPane fitToHeight="true" fitToWidth="true">
            <VBox fx:id="maincontainer" spacing="10" />
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </padding>
        </ScrollPane>
    </center>
    <bottom>
        <HBox prefHeight="100.0" prefWidth="200.0" BorderPane.alignment="CENTER">
            <children>
                <JFXButton fx:id="allbookings" text="All Bookings" />
                <JFXButton fx:id="previousbookings" text="Past Bookings" />
                <JFXButton fx:id="futurebookings" text="Future Bookings" />
            </children>
        </HBox>
    </bottom>
</BorderPane>
