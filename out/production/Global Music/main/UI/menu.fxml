<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.*?>
<?import com.jfoenix.controls.JFXButton?>

<AnchorPane fx:id="root" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1"
            stylesheets="@../Resources/Styles/main.css" fx:controller="main.Controllers.MenuController">
    <VBox prefWidth="200.0" styleClass="vbox" spacing="10"
          AnchorPane.bottomAnchor="0" AnchorPane.topAnchor="0">
        <JFXButton fx:id="bookingButton" text="Bookings" styleClass="test-label" maxWidth="Infinity" onAction="#bookingEvent"/>
        <JFXButton fx:id="accountButton" text="My Account" styleClass="test-label"  maxWidth="Infinity" onAction="#accountEvent"/>
        <JFXButton fx:id="festivalsButton" text="Festivals" styleClass="test-label" maxWidth="Infinity" onAction="#festivalEvent"/>
        <JFXButton fx:id="adminAcc" text="Admin acc" styleClass="test-label" maxWidth="Infinity" onAction="#adminAccEvent"/>
        <JFXButton text="fifth item" styleClass="test-label" maxWidth="Infinity"/>
        <JFXButton text="seventh item" styleClass="test-label" maxWidth="Infinity"/>
        <JFXButton text="eighth item" styleClass="test-label" maxWidth="Infinity"/>
    </VBox>

    <JFXButton fx:id="logOutButton" text="Log out" styleClass="test-label" prefWidth="200.0" AnchorPane.bottomAnchor="20"
            onAction="#logOut"/>
</AnchorPane>