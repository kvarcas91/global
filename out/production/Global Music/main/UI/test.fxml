
<StackPane stylesheets="@dark-theme.css" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="600.0" prefWidth="800.0" styleClass="login-bg">
            <children>
                <AnchorPane layoutX="89.0" layoutY="80.0" prefHeight="440.0" prefWidth="600.0" style="-fx-background-color: #FFFFFF; -fx-background-radius: 20px;">
                    <effect>
                        <DropShadow color="WHITE" radius="14" width="40" />
                    </effect>
                    <children>
                        <Group layoutX="133.0" layoutY="68.0">
                            <children>
                                <Pane fx:id="slidingPane" layoutX="6.0" prefHeight="35.0" prefWidth="325.0">
                                    <children>
                                        <Label fx:id="lblLogin" onMouseClicked="#openLoginTab" prefHeight="35.0" prefWidth="150.0" styleClass="lbl-register" text="Login">
                                            <cursor>
                                                <Cursor fx:constant="HAND" />
                                            </cursor>
                                        </Label>
                                        <Label fx:id="lblRegister" layoutX="150.0" prefHeight="35.0" prefWidth="150.0" styleClass="lbl-register" text="Register">
                                            <cursor>
                                                <Cursor fx:constant="HAND" />
                                            </cursor>
                                        </Label>
                                    </children>
                                </Pane>
                                <Label fx:id="lblStatus" prefHeight="35.0" prefWidth="160.0" styleClass="lbl-status" text="Login" />
                            </children>
                        </Group>
                        <TabPane fx:id="tabPaneControl" layoutX="23.0" layoutY="86.0" prefHeight="342.0" prefWidth="557.0" tabClosingPolicy="UNAVAILABLE">
                            <tabs>
                                <Tab fx:id="tabLogin" text="Login">
                                    <content>
                                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="283.0" prefWidth="558.0">
                                            <children>
                                                <JFXTextField labelFloat="true" layoutX="172.0" layoutY="50.0" prefHeight="26.0" prefWidth="200.0" promptText="Username" />
                                                <JFXButton fx:id="login" layoutX="326.0" layoutY="235.0" styleClass="btn_login" text="Login" />
                                                <JFXPasswordField labelFloat="true" layoutX="172.0" layoutY="114.0" prefHeight="26.0" prefWidth="200.0" promptText="Password" />
                                            </children>
                                        </AnchorPane>
                                    </content>
                                    <graphic>
                                        <Label prefHeight="35.0" prefWidth="150.0" styleClass="lbl-register" text="Register" />
                                    </graphic>
                                </Tab>
                                <Tab fx:id="tabRegister" text="Register">
                                    <content>
                                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
                                    </content>
                                </Tab>
                            </tabs>
                        </TabPane>
                    </children>
                </AnchorPane>
            </children></AnchorPane>
    </children>
</StackPane>
